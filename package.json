{
  "name": "Eyjatto",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "peerDependencies": {
    "prop-types": "^15",
    "react": "^16",
    "react-dom": "^16"
  },
  "scripts": {
    "cf": "echo 'cf. https://classic.yarnpkg.com/en/docs/cli/'",
    "package.json": "cat ./package.json",
    "package.json:cf": "echo 'cf. https://docs.npmjs.com/cli/v6/configuring-npm/package-json'",
    "webpack:cf": "webpack --help=verbose && echo 'cf. https://webpack.js.org/api/cli/'",
    "dev": "webpack serve --mode development --open --config ./.config/webpack.config.js",
    "build": "webpack --mode production --config ./.config/webpack.config.js",
    "test": "jest --config './.config/.jest.config.js'",
    "test:cf": "echo 'cf. https://jestjs.io/docs/en/cli'",
    "test:watch": "yarn test --watch",
    "lint": "eslint ./{lib,src,test}/**/*.{js,mjs,jsx,ts,tsx} --config './.config/.eslintrc.js' --ignore-path './.config/.eslintignore'",
    "lint:cf": "jest --debug && echo https://eslint.org/docs/user-guide/command-line-interface",
    "lint:all": "yarn lint --config './.config/.eslintrc.js'",
    "lint:fix": "yarn lint --cache --cache-location './.config/.eslintcache' --config './.config/.eslintrc.js' && yarn format",
    "lint:target": "echo ./{lib,src,test}/**/*.{js,mjs,jsx,ts,tsx}",
    "prettier": "prettier './{lib,src,test}/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.config/.prettierrc.json --ignore-path ./.config/.prettierignore",
    "prettier:cf": "echo 'cf. https://prettier.io/docs/en/cli.html'",
    "type:check": "tsc --project tsconfig.json --pretty --noEmit",
    "format": "yarn prettier --write",
    "format:check": "yarn prettier --check",
    "inspect": "echo 'please enter the `inspect:dev` or `inspect:prod`'. ",
    "inspect:cf": "echo https://neutrinojs.org/usage/#inspecting-the-generated-webpack-config",
    "inspect:dev": "echo \"module.exports = $(yarn --silent neutrino --inspect --mode development);\" > ./.config/exported-config-development.js",
    "inspect:prod": "echo \"module.exports = $(yarn --silent neutrino --inspect --mode production);\" > ./.config/exported-config-production.js"
  },
  "devDependencies": {
    "@neutrinojs/jest": "^9.5.0",
    "@neutrinojs/react-components": "^9.5.0",
    "@neutrinojs/standardjs": "^9.5.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26",
    "neutrino": "^9.5.0",
    "prettier": "2.2.1",
    "prop-types": "^15",
    "react": "^16",
    "react-dom": "^16",
    "source-map-support": "^0.5.19",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "5.13.0",
    "webpack-cli": "4.3.1",
    "webpack-dev-server": "^3"
  }
}
