{
    "$schema": "https://json.schemastore.org/package",
    "name": "@ningensei848/eyjatto",
    "version": "0.0.1-theta",
    "license": "MIT",
    "author": "Ningensei848",
    "description": "Generates arbitrary form(s) from textInput with the call \\\"Eyjatto\\\". / 「えいやっ」と言いながらフォームを生成する．",
    "repository": "https://github.com/Ningensei848/eyjatto",
    "publishConfig": {
        "access": "public"
    },
    "source": "src/index.tsx",
    "main": "dist/main.js",
    "module": "dist/module.js",
    "types": "dist/types.d.ts",
    "scripts": {
        "dev": "parcel ./index.html -p 3000",
        "build": "rm -rf ./dist/ && parcel build",
        "build:prod": "yarn build --no-source-maps",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx,.css,.md,.json -c .eslintrc.js 'src/**'",
        "format": "prettier './{src,test,script,public,config}/**/*.{js,jsx,ts,tsx,css,md,json,html}' --write",
        "check": "tsc --noEmit"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.6.2",
        "@swc/helpers": "^0.2.14",
        "ajv": "^8.8.0",
        "axios": "^0.24.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.6",
        "regenerator-runtime": "^0.13.9",
        "sparqljs": "^3.5.1",
        "swr": "^1.0.1"
    },
    "devDependencies": {
        "@parcel/packager-ts": "^2.0.1",
        "@parcel/reporter-bundle-analyzer": "^2.0.1",
        "@parcel/transformer-typescript-types": "^2.0.1",
        "@types/node": "^16.11.9",
        "@types/react": "^17.0.35",
        "@types/react-dom": "^17.0.11",
        "@types/react-redux": "^7.1.7",
        "@types/sparqljs": "^3.1.3",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jest": "^25.2.4",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "parcel": "^2.0.1",
        "prettier": "^2.4.1",
        "typescript": "^4.5.2"
    },
    "engines": {
        "node": ">= 16"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    },
    "eslintIgnore": [
        ".eslintrc.js",
        "/.parcel-cache/"
    ]
}
